nav-menu active\" : \"nav-menu\"}>\r\n            <li>\r\n              <Link\r\n                to=\"/login\"\r\n                className=\"nav-links-mobile\"\r\n                onClick={closeMobileMenu}\r\n              >\r\n                <i class=\"fas fa-user\"></i>\r\n                <br />\r\n                Log ind\r\n              </Link>\r\n            </li>\r\n            <li>\r\n              <Link\r\n                to=\"/cart\"\r\n                className=\"nav-links-mobile\"\r\n                onClick={closeMobileMenu}\r\n              >\r\n                <i class=\"fas fa-shopping-cart\"></i>\r\n                <br />\r\n                {cart != 0 && totalPrice + deliveryCost + currency}\r\n              </Link>\r\n            </li>\r\n          </ul>\r\n          {!localStorage.getItem(\"loginEmail\") && button && (\r\n            <Button buttonStyle=\"btn--outline\">\r\n              {\" \"}\r\n              <i class=\"fas fa-user\"></i> {buttonText}\r\n            </Button>\r\n          )}\r\n          {localStorage.getItem(\"loginEmail\") && button && (\r\n            <ButtonProfile buttonStyle=\"btn--outline\">\r\n              {currentUser && currentUser.email}\r\n            </ButtonProfile>\r\n          )}\r\n\r\n          {button && (\r\n            <ButtonCart onClick={handleCartClick} buttonStyle=\"btn--outline\">\r\n              {\" \"}\r\n              {cart != 0 && cart.length}\r\n              <i class=\"fas fa-shopping-cart\"></i>{\" \"}\r\n              {cart != 0 && totalPrice + deliveryCost + currency}\r\n            </ButtonCart>\r\n          )}\r\n        </div>\r\n      </nav>\r\n    </>\r\n  );\r\n}\r\n\r\nexport default Navbar;\r\n","C:\\Users\\marti\\Documents\\react\\myreactproject\\src\\Components\\pages\\Home.js",[],"C:\\Users\\marti\\Documents\\react\\myreactproject\\src\\Components\\Cards.js",["160","161","162","163","164"],"import React, { useState } from \"react\";\r\nimport \"./Cards.css\";\r\nimport CardItem fro